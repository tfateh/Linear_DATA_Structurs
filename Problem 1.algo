ALGORITHM sum_of_all_distinct
VAR
    tab:ARRAY_OF integer[];
    TAB:ARRAY_OF integer[];
    i,j,Sum:integer;
BEGIN
   FOR i FROM 0 TO tab.length-1 DO 
   read(tab[i]);
   END FOR
   FOR j FROM 0 TO TAB.length-1 DO
   read(tab[j]);
   END FOR
   
   IF (i:=j) THEN
       Sum:=tab[i]+TAB[j];
   ELSE
       i++;
   END_IF
END